{
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 50,
        "tabWidth": 2,
        "useTabs": false,
        "semi": false,
        "singleQuote": true,
        "trailingComma": "es5",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "arrowParens": "always",
        "proseWrap": "never",
        "htmlWhitespaceSensitivity": "ignore",
        "endOfLine": "auto"
      }
    }
  ]
}
